<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="lab05.City" table="CITY"/>
        <id name="locId">  
            <generator class="assigned"></generator>  
        </id>                  
        <property name="country"></property>  
        <property name="region"></property> 
        <property name="postalCode"></property>  
        <property name="latitude"></property> 
        <property name="longitude"></property>  
        <property name="metroCode"></property> 
        <property name="areaCode"></property> 
</hibernate-mapping>






